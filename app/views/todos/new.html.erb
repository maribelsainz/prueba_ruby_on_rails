<h1>Ingresa TO DO!</h1>
<br>
<br>

<%= form_with(model: @todo, local: true, class: 'form-inline') do |f| %>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :completed %>
    <%= f.check_box :completed %>
  </div>

  <div>
    <%= f.submit 'Enviar' %>
  </div>


<% end %>